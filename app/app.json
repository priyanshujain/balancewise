{
  "expo": {
    "name": "BalanceWise",
    "slug": "balancewise",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "BalanceWise",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
          "NSCameraUsageDescription": "BalanceWise needs access to your camera to capture",
          "NSPhotoLibraryUsageDescription": "BalanceWise needs access to your photo library"
      },
      "bundleIdentifier": "com.priyanshujain.balancewise"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FF7043",
        "foregroundImage": "./assets/images/app-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.priyanshujain.balancewise"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/app-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FF7043",
          "dark": {
            "backgroundColor": "#FF7043"
          }
        }
      ],
      [
        "expo-image-picker",
        {
            "photosPermission": "BalanceWise needs access to your photo library.",
            "cameraPermission": "BalanceWise needs access to your camera"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e14a73df-e01a-438c-b865-750ecf3e2bfb"
      }
    },
    "owner": "priyanshujain"
  }
}